SELECT `follow_id` FROM `yd_user_follow` WHERE `is_valid`=1 AND `leader_id` ="30646"

[ @sqlPost@ ]  2016-04-15 23:46:05
SELECT A.post_id,A.leader_id,B.user_name AS poster_name,B.avatar AS poster_avatar,A.post_time,A.content AS post_content
                    FROM `yd_post` AS A
                    LEFT JOIN `yd_user` AS B ON A.leader_id = B.leader_id
                    WHERE A.leader_id  in (30646) AND A.post_id <"1" ORDER BY A.post_time DESC LIMIT 0,5

